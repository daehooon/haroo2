<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bit189.haroo.dao.AttachedFileDao">

  <insert id="insert" parameterType="AttachedFile" useGeneratedKeys="true" keyColumn="pno" keyProperty="no">
    insert into har_srpl_file (pno, srpl_fname)
    values (#{postNo}, #{name})
  </insert>

</mapper> 